#!/bin/bash

# make a folder called files and put all the files in it. 
# copy this file into the same folder as 'files' folder ( NOT inside it )
# go to the terminal and cd to the folder where this script is
# type chmod +x run in the terminal
 
mkdir target || (echo "a folder called target should not exist" && exit 1)
for file in $(ls ./files/)
do
targetName=`echo "$file" | awk 'BEGIN{FS="."}{print $1}'`
tr 'A-Z' 'a-z' < ./files/$file | tr -cs '[a-z]' '\012' | awk '{ printf("%s ",$1)} END{printf("\n")} ' > ./target/$targetName".dat"
done

"#!/bin/bash

# make a folder called files and put all the files in it. 
# copy this file into the same folder as 'files' folder ( NOT inside it )
# go to the terminal and cd to the folder where this script is
# type chmod +x run in the terminal
 
rm f 2> /dev/null
touch f
for file in $(ls ./files/)
do
	tr 'A-Z' 'a-z' < ./files/$file | tr -cs '[a-z]' '\012' | awk '{ printf(""%s "",$1)} END{printf(""\n"")} ' >> f
done
"


